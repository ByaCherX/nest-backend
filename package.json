{
  "name": "nest-server-app",
  "version": "1.0.0",
  "description": "Backend server built with nestjs",
  "author": "EmreKayal",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -c .\\nest-cli.json",
    "build:swc": "swc --out-dir dist src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -c .\\nest-cli.json",
    "start:dev": "nest start --watch -c .\\nest-cli.json",
    "start:debug": "nest start --debug --watch -c .\\nest-cli.json",
    "start:profile": "node --prof dist/main.js",
    "start:prod": "node dist/main",
    "start:cluster": "echo no-added, manually run > cluster.service.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.1",
    "@node-rs/bcrypt": "^1.6.2",
    "@types/compression": "^1.7.2",
    "ansi-colors": "^4.1.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "bullmq": "^3.2.4",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "eventemitter2": "^6.4.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fastify": "^4.28.1",
    "helmet": "^6.0.0",
    "hyper-express": "^6.14.8",
    "jest-mock": "^29.7.0",
    "mariadb": "^3.2.3",
    "mysql2": "^3.11.0",
    "nest-winston": "^1.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.6.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.5.4",
    "spdy": "^4.0.2",
    "ts-proto": "^1.156.4",
    "typeorm": "^0.3.19",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/spdy": "^3.4.5",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autocannon": "^7.14.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "jsx"
    ],
    "rootDir": "./test",
    "modulePaths": [
      "$(ProjectDir)/node_modules/",
      "<rootDir>../"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
